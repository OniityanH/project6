\
/**
 * 8-Way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0],b=in[1],out=a);
    Or(a=a,b=in[2],out=b);
    Or(a=b,b=in[3],out=c);
    Or(a=c,b=in[4],out=d);
    Or(a=d,b=in[5],out=e);
    Or(a=e,b=in[6],out=f);
    Or(a=f,b=in[7],out=out);
}